<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="pantallaLogin" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.screens.login.LoginController">

        <TextField fx:id="txtUserName" layoutX="225.0" layoutY="151.0" prefHeight="50.0" prefWidth="119.0" promptText="user" />
        <PasswordField fx:id="txtPassword" layoutX="225.0" layoutY="210.0" prefHeight="19.0" prefWidth="119.0" promptText="password" />
        <Button fx:id="btLogin" layoutX="207.0" layoutY="265.0" mnemonicParsing="false" onAction="#doLogin" prefHeight="26.0" prefWidth="119.0" text="Login" />

    <Button fx:id="btnLoad" layoutX="351.0" layoutY="265.0" mnemonicParsing="false" onAction="#loadData" prefHeight="26.0" prefWidth="119.0" text="Load Data" />
</AnchorPane>
